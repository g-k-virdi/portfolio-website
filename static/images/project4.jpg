<svg width="800" height="500" xmlns="http://www.w3.org/2000/svg">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2d3142;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#4f5d75;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Main background -->
  <rect width="100%" height="100%" fill="url(#bg-gradient)" />
  
  <!-- Hockey rink outline -->
  <rect x="150" y="150" width="500" height="250" rx="100" stroke="#bfc0c0" stroke-width="4" fill="none" />
  <line x1="400" y1="150" x2="400" y2="400" stroke="#bfc0c0" stroke-width="2" />
  <circle cx="400" cy="275" r="50" stroke="#bfc0c0" stroke-width="2" fill="none" />
  
  <!-- Hockey stick and puck -->
  <path d="M300,300 C320,320 350,330 380,320" stroke="#ef8354" stroke-width="6" fill="none" />
  <rect x="280" y="290" width="20" height="80" rx="5" fill="#ef8354" />
  <circle cx="390" cy="320" r="10" fill="#bfc0c0" />
  
  <!-- Data visualization elements -->
  <rect x="550" y="200" width="150" height="15" rx="5" fill="#ef8354" opacity="0.7" />
  <rect x="550" y="230" width="100" height="15" rx="5" fill="#ef8354" opacity="0.5" />
  <rect x="550" y="260" width="120" height="15" rx="5" fill="#ef8354" opacity="0.6" />
  <rect x="550" y="290" width="80" height="15" rx="5" fill="#ef8354" opacity="0.4" />
  <rect x="550" y="320" width="130" height="15" rx="5" fill="#ef8354" opacity="0.65" />
  
  <!-- Labels -->
  <text x="530" y="210" font-family="Arial" font-size="12" fill="#bfc0c0" text-anchor="end">Player A</text>
  <text x="530" y="240" font-family="Arial" font-size="12" fill="#bfc0c0" text-anchor="end">Player B</text>
  <text x="530" y="270" font-family="Arial" font-size="12" fill="#bfc0c0" text-anchor="end">Player C</text>
  <text x="530" y="300" font-family="Arial" font-size="12" fill="#bfc0c0" text-anchor="end">Player D</text>
  <text x="530" y="330" font-family="Arial" font-size="12" fill="#bfc0c0" text-anchor="end">Player E</text>
  
  <!-- Project title -->
  <text x="400" y="70" font-family="Arial" font-size="24" fill="#ffffff" text-anchor="middle">NHL Draft Prediction</text>
  <text x="400" y="100" font-family="Arial" font-size="16" fill="#bfc0c0" text-anchor="middle">Machine Learning Analysis</text>
</svg> 