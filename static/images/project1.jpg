<svg width="800" height="500" xmlns="http://www.w3.org/2000/svg">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#4f5d75;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2d3142;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Main background -->
  <rect width="100%" height="100%" fill="url(#bg-gradient)" />
  
  <!-- UX Design elements -->
  <rect x="100" y="100" width="600" height="300" rx="10" fill="#ffffff" opacity="0.1" />
  
  <!-- Wireframe elements -->
  <rect x="150" y="150" width="200" height="50" rx="5" fill="#bfc0c0" opacity="0.3" />
  <rect x="150" y="220" width="200" height="150" rx="5" fill="#bfc0c0" opacity="0.3" />
  
  <rect x="400" y="150" width="250" height="30" rx="5" fill="#bfc0c0" opacity="0.3" />
  <rect x="400" y="200" width="250" height="30" rx="5" fill="#bfc0c0" opacity="0.3" />
  <rect x="400" y="250" width="250" height="30" rx="5" fill="#bfc0c0" opacity="0.3" />
  <rect x="400" y="300" width="150" height="40" rx="5" fill="#ef8354" opacity="0.5" />
  
  <!-- UX Design text -->
  <text x="400" y="450" font-family="Arial" font-size="24" fill="#ffffff" text-anchor="middle">UX Design Project</text>
</svg> 