<svg width="800" height="500" xmlns="http://www.w3.org/2000/svg">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#4f5d75;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2d3142;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Main background -->
  <rect width="100%" height="100%" fill="url(#bg-gradient)" />
  
  <!-- Graph elements -->
  <polyline points="100,350 200,300 300,320 400,250 500,200 600,230 700,180" 
          stroke="#ef8354" stroke-width="4" fill="none" />
  
  <!-- Data points -->
  <circle cx="100" cy="350" r="6" fill="#ef8354" />
  <circle cx="200" cy="300" r="6" fill="#ef8354" />
  <circle cx="300" cy="320" r="6" fill="#ef8354" />
  <circle cx="400" cy="250" r="6" fill="#ef8354" />
  <circle cx="500" cy="200" r="6" fill="#ef8354" />
  <circle cx="600" cy="230" r="6" fill="#ef8354" />
  <circle cx="700" cy="180" r="6" fill="#ef8354" />
  
  <!-- Axes -->
  <line x1="100" y1="100" x2="100" y2="400" stroke="#bfc0c0" stroke-width="2" />
  <line x1="100" y1="400" x2="700" y2="400" stroke="#bfc0c0" stroke-width="2" />
  
  <!-- Axis labels -->
  <text x="400" y="430" font-family="Arial" font-size="16" fill="#bfc0c0" text-anchor="middle">Time</text>
  <text x="70" y="250" font-family="Arial" font-size="16" fill="#bfc0c0" text-anchor="middle" transform="rotate(-90, 70, 250)">Pollution Level</text>
  
  <!-- Grid lines -->
  <line x1="100" y1="350" x2="700" y2="350" stroke="#bfc0c0" stroke-width="1" opacity="0.2" />
  <line x1="100" y1="300" x2="700" y2="300" stroke="#bfc0c0" stroke-width="1" opacity="0.2" />
  <line x1="100" y1="250" x2="700" y2="250" stroke="#bfc0c0" stroke-width="1" opacity="0.2" />
  <line x1="100" y1="200" x2="700" y2="200" stroke="#bfc0c0" stroke-width="1" opacity="0.2" />
  <line x1="100" y1="150" x2="700" y2="150" stroke="#bfc0c0" stroke-width="1" opacity="0.2" />
  
  <line x1="200" y1="100" x2="200" y2="400" stroke="#bfc0c0" stroke-width="1" opacity="0.2" />
  <line x1="300" y1="100" x2="300" y2="400" stroke="#bfc0c0" stroke-width="1" opacity="0.2" />
  <line x1="400" y1="100" x2="400" y2="400" stroke="#bfc0c0" stroke-width="1" opacity="0.2" />
  <line x1="500" y1="100" x2="500" y2="400" stroke="#bfc0c0" stroke-width="1" opacity="0.2" />
  <line x1="600" y1="100" x2="600" y2="400" stroke="#bfc0c0" stroke-width="1" opacity="0.2" />
  <line x1="700" y1="100" x2="700" y2="400" stroke="#bfc0c0" stroke-width="1" opacity="0.2" />
  
  <!-- Project title -->
  <text x="400" y="70" font-family="Arial" font-size="24" fill="#ffffff" text-anchor="middle">Pollution Forecasting</text>
</svg> 