<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2d3142;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#4f5d75;stop-opacity:1" />
    </linearGradient>
    
    <!-- Abstract pattern -->
    <pattern id="pattern" width="100" height="100" patternUnits="userSpaceOnUse">
      <circle cx="50" cy="50" r="20" fill="#ef8354" opacity="0.2" />
      <rect x="20" y="20" width="60" height="60" fill="none" stroke="#bfc0c0" stroke-width="2" opacity="0.1" />
    </pattern>
  </defs>
  
  <!-- Main background -->
  <rect width="100%" height="100%" fill="url(#bg-gradient)" />
  
  <!-- Pattern overlay -->
  <rect width="100%" height="100%" fill="url(#pattern)" />
  
  <!-- Abstract portrait silhouette -->
  <ellipse cx="400" cy="250" rx="150" ry="180" fill="#bfc0c0" opacity="0.3" />
  <ellipse cx="400" cy="180" rx="80" ry="90" fill="#bfc0c0" opacity="0.3" />
  
  <!-- Text placeholder -->
  <rect x="200" y="450" width="400" height="30" rx="5" fill="#bfc0c0" opacity="0.3" />
  <rect x="250" y="500" width="300" height="20" rx="5" fill="#bfc0c0" opacity="0.3" />
  
  <!-- Decorative elements -->
  <circle cx="150" cy="150" r="50" fill="#ef8354" opacity="0.1" />
  <circle cx="650" cy="450" r="70" fill="#ef8354" opacity="0.1" />
  <rect x="100" y="350" width="100" height="100" rx="10" fill="#bfc0c0" opacity="0.1" />
  <rect x="600" y="100" width="80" height="80" rx="10" fill="#bfc0c0" opacity="0.1" />
</svg> 